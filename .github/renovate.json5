{
  "extends": [
    ":enableRenovate",
    "config:base",
    ":disableRateLimiting",
    ":dependencyDashboard",
    "docker:enableMajor",
    ":enablePreCommit",
  ],
  hostRules: [
    {
      hostType: "docker",
      username: "nick170",
      encrypted: {
        password: ""
      }
    }
  ],
  "platform": "github",
  "username": "nicko170",
  "repositories": [
    "nicko170/infra"
  ],
  "onboarding": false,
  "requireConfig": false,
  "gitAuthor": "DevLAN-io[bot] <172667+DevLAN-io[bot]@users.noreply.github.com>",
  "dependencyDashboardTitle": "Renovate Dashboard ðŸ¤–",
  "suppressNotifications": [
    "prIgnoreNotification"
  ],
  "rebaseWhen": "conflicted",
  "ignorePaths": [
    "cluster/base/",
    "**/backup-job.yaml",
    "**/trash-updater-job.yaml"
  ],
  // set up renovate managers
  "docker-compose": {
    "fileMatch": [
      "(^|\/)docker-compose.*\.ya?ml$",
      "(^|\/)docker-compose.*\.ya?ml\.j2$"
    ]
  },
  "helm-values": {
    "fileMatch": [
      "clusters/.+/helm-release\\.yaml$",
      "clusters/.+/helm-release\\.yaml$"
    ]
  },
  "helmv3": {
    "fileMatch": [
      "charts/.+/Chart\\.yaml$"
    ]
  },
  "kubernetes": {
    "fileMatch": [
      "cluster/.+\\.yaml$",
      "clusters/.+\\.yaml$"
    ]
  },
  "flux": {
    "fileMatch": [
      "cluster/.+\\.yaml$",
      "clusters/.+\\.yaml$",
    ]
  },
  "packageRules": [
    {
      matchUpdateTypes: [
        "minor",
        "patch",
        "pin",
        "digest"
      ],
      automerge: true
    },
    {
      "matchDatasources": [
        "helm"
      ],
      "ignoreDeprecated": true
    },
    {
      "matchDatasources": [
        "docker"
      ],
      "commitMessageExtra": "to {{newVersion}}"
    }
  ]
}
